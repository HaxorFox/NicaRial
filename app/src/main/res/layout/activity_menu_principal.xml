        android:layout_alignStart="@+id/imageView13" />

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/language"
        android:id="@+id/imageView19"
        android:layout_alignTop="@+id/imageView17"
        android:layout_alignStart="@+id/imageView15" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/shutdown"
        android:id="@+id/imageView9"
        android:layout_below="@+id/imageView17"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp" />

    <ImageView
        android:layout_width="50dp"
        app:srcCompat="@drawable/like"
        android:id="@+id/imageView7"
        android:layout_height="50dp"
        android:layout_marginEnd="15dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageView6" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/thumbsdown"
        android:id="@+id/imageView6"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageView2"
        android:layout_marginEnd="7dp" />

    <TextView
        android:text="MenÃº Principal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_marginTop="15dp"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
